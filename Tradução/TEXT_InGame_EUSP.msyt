  "[500EC6A0]_GrappleBeamDescription":
    attributes: ""
    contents:
      - text: "O "
      - control:
          kind: raw
          zero:
            three:
              field_1: 4
              field_2: 54921
      - text: ￿Gancho Magnético
      - control:
          kind: raw
          zero:
            three:
              field_1: 4
              field_2: 0
      - text: "＀ permite você balançar em pontos especiais no ambiente.  \n\nLugares para usar o gancho aparecem em seu visor como "
      - control:
          kind: raw
          one:
            one:
              field_1: 38
              field_2: 5505058
      - text: "XTR_GlyphGrapple\u0001.\n\nSegure "
      - control:
          kind: raw
          one:
            zero:
              field_1: 2
              field_2: 2149643
      - text: "para lançar o Gancho Magnético.\n\nSegure "
      - control:
          kind: raw
          one:
            zero:
              field_1: 2
              field_2: 2149643
      - text: "para ficar conectado; solte para se soltar.\n\n"
      - control:
